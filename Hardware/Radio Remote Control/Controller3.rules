
(rules PCB Controller3
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1790)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.2)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.5)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-battery" "Via[0-1]_800:400_um" battery
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    battery "Via[0-1]_800:400_um-battery"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(BT1-Pad2)" "Net-(U1-Pad23)" "Net-(U1-Pad24)" "Net-(U1-Pad25)" "Net-(U1-Pad26)" "Net-(U1-Pad13)" "Net-(U1-Pad27)" "Net-(U1-Pad28)"
    "Net-(U2-PadS1)" "Net-(U3-PadS1)" "Net-(SW1-Pad1)" "Net-(SW2-Pad2)" "Net-(SW3-Pad2)" "Net-(U1-Pad11)" "Net-(U1-Pad12)" "Net-(U1-Pad14)"
    "Net-(U2-PadB1A)" "Net-(U2-PadB2A)" "Net-(U3-PadB1A)" "Net-(U3-PadB2A)" "Net-(U4-Pad8)" "Net-(J1-Pad1)" "Net-(J1-Pad2)" "Net-(J1-Pad3)"
    "Net-(J4-Pad3)" "Net-(J4-Pad4)" "Net-(D1-Pad1)" "Net-(D1-Pad2)" "Net-(R3-Pad1)" "Net-(R3-Pad2)" "Net-(C6-Pad2)" "Net-(U1-Pad1)"
    "Net-(U1-Pad9)" "Net-(U1-Pad10)" "Net-(U1-Pad19)" "Net-(U1-Pad22)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class battery
    +BATT GND +3V3 +5V
    (clearance_class battery)
    (via_rule battery)
    (rule
      (width 550.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)